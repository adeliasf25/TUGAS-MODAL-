<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".PaketLiburanFragment"
    android:background="@color/bgapp"
    android:id="@+id/paketliburan">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingStart="10dp"
        android:paddingEnd="10dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:id="@+id/const1"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">
        <EditText
            android:id="@+id/search"
            android:layout_width="290dp"
            android:layout_height="50dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/search_background"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>

        <ImageView
            android:id="@+id/imgsearch"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_marginTop="15dp"
            android:src="@drawable/search"
            app:layout_constraintEnd_toEndOf="@+id/search"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imgnotif"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/notif"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/const1">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">



            <TextView
                android:id="@+id/textDesaWisata"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Desa Wisata"
                app:layout_constraintTop_toTopOf="@+id/search"
                app:layout_constraintStart_toStartOf="parent"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:textSize="24sp"
                android:layout_marginTop="15dp"
                android:layout_marginStart="20dp"/>

            <androidx.cardview.widget.CardView
                android:id="@+id/recyclerview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                app:cardCornerRadius="25dp"
                app:layout_constraintTop_toBottomOf="@+id/textDesaWisata"
                >
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="192dp"
                    android:background="@drawable/destinasi1"
                    >
                    <TextView
                        android:id="@+id/textjudul1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Bali Handra Gate"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="30sp"
                        android:layout_marginStart="20dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="90dp"/>
                    <TextView
                        android:id="@+id/textjudulbawah1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Bali Handra Gate"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="20sp"
                        android:layout_marginStart="20dp"
                        app:layout_constraintTop_toBottomOf="@+id/textjudul1"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="15dp"/>





                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/textDesaWisata1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Desa Wisata"
                app:layout_constraintTop_toBottomOf="@+id/recyclerview"
                app:layout_constraintStart_toStartOf="parent"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:textSize="24sp"
                android:layout_marginTop="15dp"
                android:layout_marginStart="20dp"/>
            <androidx.cardview.widget.CardView
                android:id="@+id/recyclerview2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                app:cardCornerRadius="25dp"
                app:layout_constraintTop_toBottomOf="@+id/textDesaWisata1"
                >
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="192dp"
                    android:background="@drawable/destinasi1"
                    >
                    <TextView
                        android:id="@+id/textjudul2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Bali Handra Gate"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="30sp"
                        android:layout_marginStart="20dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="90dp"/>
                    <TextView
                        android:id="@+id/textjudulbawah2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Bali Handra Gate"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="20sp"
                        android:layout_marginStart="20dp"
                        app:layout_constraintTop_toBottomOf="@+id/textjudul2"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="15dp"/>





                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/textDesaWisata3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Desa Wisata"
                app:layout_constraintTop_toBottomOf="@+id/recyclerview2"
                app:layout_constraintStart_toStartOf="parent"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:textSize="24sp"
                android:layout_marginTop="15dp"
                android:layout_marginStart="20dp"/>
            <androidx.cardview.widget.CardView
                android:id="@+id/recyclerview1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                app:cardCornerRadius="25dp"
                app:layout_constraintTop_toBottomOf="@+id/textDesaWisata3"
                >
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="192dp"
                    android:background="@drawable/destinasi1"
                    >
                    <TextView
                        android:id="@+id/textjudul3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Bali Handra Gate"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="30sp"
                        android:layout_marginStart="20dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="90dp"/>
                    <TextView
                        android:id="@+id/textjudulbawah3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Bali Handra Gate"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="20sp"
                        android:layout_marginStart="20dp"
                        app:layout_constraintTop_toBottomOf="@+id/textjudul3"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="15dp"/>





                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>


    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>